    <title>App Store</title>
    <style>
        /* use grid */
        .header { grid-area: header; }
        .menu { grid-area: menu; }
        .main { grid-area: main; }
        .right { grid-area: right; }
        .footer { grid-area: footer; }

        .main {
            margin-bottom: 15px;
        }
        
        ul {
            list-style-type: none;
            margin: 0;
            padding: 0;
        }

        .grid-container {
            display: grid;
            grid-template-areas: 'header header header header header header'
                                 'main main main main main main'
                                 'footer footer footer footer footer footer';
            gap: auto;
            background-color: none;
            padding: 0px;
        }

            .grid-container > div {
                background-color: none;
                text-align: center;
                padding: 14px 0px;
                font-size: 31px;
                padding-left: 5px;
            }

        .scrollmenu {
            background-color: #999;
            overflow: auto;
            white-space: nowrap;
            width: 100%;
            margin: 0;   
        }

            div.scrollmenu a {             
                display: inline-block;
                color: #333;
                text-align: center;
                padding: 14px;
                text-decoration: none;
            }

            div.scrollmenu a:hover {
                background-color: #777;
            }

        .grid-container {
            display: grid;
            justify-content: center;
            grid-template-columns: 100%;
            gap: auto;
            background-color: none;
            padding: 0px;
        }

            .grid-container > div {
                background-color: none;
                text-align: center;
                padding: 14px 0;
                font-size: 14px;
            }

                .item1 {
                    grid-row: 1 / 4;
                }   
         
            
        /* Add a background color to the top navigation */
        .topnav {
            background-color: #333;
            overflow: hidden;
            width: 100%;
            position: fixed; 
            top: 0;
            left: 1px;
            right: 1px;
        }

            /* Style the links inside the navigation bar */
            .topnav a {
                float: left;
                color: #f2f2f2;
                text-align: center;
                padding: 14px 16px;
                text-decoration: none;
                font-size: 14px;
                
            }

            /* Change the color of links on hover */
            .topnav a:hover {
                background-color: #ddd;
                color: black;
            }

            /* Add a color to the active/current link */
            .topnav a.active {
                background-color: #04AA6D;
                color: white;
            }

        /* Add a black background color to the bottom navigation */
        .botnav {
            background-color: #333;
            width: 100%;
            overflow: hidden;
            position: fixed; 
            bottom: 0;
            left: 1px;
            right: 1px;
        }

            /* Style the links inside the navigation bar */
            .botnav a {
                float: left;
                color: #f2f2f2;
                text-align: center;
                padding: 14px 0px;
                text-decoration: none;
                font-size: 17px;
                width: 20%;
                
            }

            /* Change the color of links on hover */
            .botnav a:hover {
                background-color: #ddd;
                color: black;
            }

            /* Add a color to the active/current link */
            .botnav a.active {
                background-color: #04AA6D;
                color: white;
            }

            body {
                background-color: pink;
            }

            @media screen and (min-width: 480px) {
                body {
                    background-color: white;
                }
            }
        
        /* Pagination links */
        .pagination a {
            color: black;
            float: left;
            padding: 8px 16px;
            text-decoration: none;
            transition: background-color .3s;
        }

        /* Style the active/current link */
        .pagination a.active {
            background-color: dodgerblue;
            color: white;
        }

        /* Add a grey background color on mouse-over */
        .pagination a:hover:not(.active) {background-color: #ddd;}
    </style>
</head>
